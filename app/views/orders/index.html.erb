<h1>Orders#index</h1>
<p>Find me in app/views/orders/index.html.erb</p>






<%= form_with model: @order do |form| %>
  <div class="container-flex">
  <div class="field">
    <%= form.label :image %><br>
    <%= form.file_field :image_filename %>
    <% @order.errors.full_messages_for(:image_filename).each do |message| %>
          <div><%= message %></div>
      <% end %>
  </div>




  <div class="form_btn">
    <%= form.submit 'Save' %>
  </div>
<% end %>
